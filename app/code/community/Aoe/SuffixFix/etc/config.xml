<?xml version="1.0"?>
<!--
/**
 * Aoe_SuffixFix
 *
 * @author Fabrizio Branca
 */
-->
<config>
	<modules>
		<Aoe_SuffixFix>
			<version>0.0.1</version>
		</Aoe_SuffixFix>
	</modules>
	<global>
		<helpers>
			<aoe_suffixfix>
				<class>Aoe_SuffixFix_Helper</class>
			</aoe_suffixfix>
			<catalog>
				<rewrite>
					<category>Aoe_SuffixFix_Helper_Category</category>
					<product>Aoe_SuffixFix_Helper_Product</product>
				</rewrite>
			</catalog>
			<enterprise_catalog>
				<rewrite>
					<data>Aoe_SuffixFix_Helper_EnterpriseCatalogHelperData</data>
				</rewrite>
			</enterprise_catalog>
		</helpers>
        <models>
            <aoe_suffixfix>
                <class>Aoe_SuffixFix_Model</class>
            </aoe_suffixfix>
            <sitemap_resource>
                <rewrite>
                    <catalog_product>Aoe_SuffixFix_Model_Resource_Catalog_Product</catalog_product>
                    <catalog_category>Aoe_SuffixFix_Model_Resource_Catalog_Category</catalog_category>
                </rewrite>
            </sitemap_resource>
        </models>
	</global>
</config>
